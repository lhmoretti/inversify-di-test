{
    "name": "di-test-back",
    "version": "1.0.0",
    "description": "",
    "main": "app.js",
    "scripts": {
        "build": "tsc",
        "dev": "nodemon exec",
        "prod": "pm2 start dist/app.js",
        "start:dev": "npm run build && npm run dev",
        "start:prod": "npm run build && npm run prod",
        "db:up": "npx typeorm-ts-node-commonjs migration:generate src/database/migrations/init -d src/data.source.sql && npx typeorm-ts-node-commonjs migration:run -d src/data.source.sql",
        "db:down": "npx typeorm-ts-node-commonjs schema:drop -d src/data.source.sql",
        "db:reset": "rm -R src/database/migrations && npm run db:down && npm run db:up",
        "db:migrate": "npx typeorm-ts-node-commonjs migration:run -d src/data.source.sql",
        "test": "jest --coverage",
        "test:watch": "jest --watch"
    },
    "author": "Lucas Moretti",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.1.0",
        "bluebird": "^3.7.2",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dayjs": "^1.11.7",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "fast-csv": "^4.3.6",
        "helmet": "^6.0.1",
        "inversify": "^6.0.1",
        "inversify-binding-decorators": "^4.0.0",
        "jsonwebtoken": "^9.0.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "node-cron": "^3.0.2",
        "node-fetch": "^3.3.1",
        "nodemailer": "^6.9.1",
        "pg": "^8.9.0",
        "pino": "^8.11.0",
        "reflect-metadata": "^0.1.13",
        "ts-node": "^10.9.1",
        "typeorm": "^0.3.12"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.0",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/node": "^18.14.1",
        "@types/nodemailer": "^6.4.7",
        "@types/request": "^2.48.8",
        "@typescript-eslint/eslint-plugin": "^5.53.0",
        "@typescript-eslint/parser": "^5.53.0",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "faker": "^5.5.3",
        "http-status-codes": "^2.2.0",
        "nodemon": "^2.0.20",
        "prettier": "2.8.4",
        "supertest": "^6.3.3",
        "typescript": "^4.9.5"
    }
}
